#: ui/main.qml:52
#, kde-format
msgctxt "@label:textbox"
msgid "Desktop folder:"
msgstr "Cesta k ploche:"

#: ui/main.qml:58
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder contains all the files you see on your desktop."
msgstr ""
"Tento priečinok obsahuje všetky súbory, ktoré vidíte na pracovnej ploche."

#: ui/main.qml:66
#, kde-format
msgctxt "@label:textbox"
msgid "Documents folder:"
msgstr "Cesta k dokumentom:"

#: ui/main.qml:72
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save documents."
msgstr ""
"Tento priečinok sa bude predvolene používať na otváranie alebo ukladanie "
"dokumentov."

#: ui/main.qml:80
#, kde-format
msgctxt "@label:textbox"
msgid "Downloads folder:"
msgstr "Cesta k preberaniam"

#: ui/main.qml:86
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to save your downloaded items."
msgstr ""
"Tento priečinok sa predvolene používa na ukladanie prevzatých položiek."

#: ui/main.qml:94
#, kde-format
msgctxt "@label:textbox"
msgid "Videos folder:"
msgstr "Cesta k videám:"

#: ui/main.qml:100
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save video files."
msgstr ""
"Tento priečinok sa bude predvolene používať na otváranie alebo ukladanie "
"videosúborov."

#: ui/main.qml:108
#, kde-format
msgctxt "@label:textbox"
msgid "Pictures folder:"
msgstr "Cesta k obrázkom:"

#: ui/main.qml:114
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save image files."
msgstr ""
"Tento priečinok sa bude predvolene používať na otváranie alebo ukladanie "
"obrázkov."

#: ui/main.qml:122
#, kde-format
msgctxt "@label:textbox"
msgid "Music folder:"
msgstr "Cesta k hudbe:"

#: ui/main.qml:128
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save music files."
msgstr ""
"Tento priečinok sa bude predvolene používať na otváranie alebo ukladanie "
"hudobných súborov."

#: ui/main.qml:136
#, kde-format
msgctxt "@label:textbox"
msgid "Public folder:"
msgstr "Cesta k verejným súborom:"

#: ui/main.qml:142
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid ""
"This folder will be used by default for publicly-shared files when network "
"sharing is enabled."
msgstr ""
"Tento priečinok sa bude predvolene používať pre verejne zdieľané súbory, keď "
"je povolené sieťové zdieľanie."

#: ui/main.qml:150
#, kde-format
msgctxt "@label:textbox"
msgid "Templates folder:"
msgstr "Cesta k šablónam:"

#: ui/main.qml:156
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save file templates."
msgstr ""
"Tento priečinok sa bude predvolene používať na otváranie alebo ukladanie "
"šablón."

