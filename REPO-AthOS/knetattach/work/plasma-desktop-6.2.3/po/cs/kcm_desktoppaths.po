#: ui/main.qml:52
#, kde-format
msgctxt "@label:textbox"
msgid "Desktop folder:"
msgstr "Složka plochy:"

#: ui/main.qml:58
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder contains all the files you see on your desktop."
msgstr "Tato složka obsahuje všechny soubory, které uvidíte na pracovní ploše."

#: ui/main.qml:66
#, kde-format
msgctxt "@label:textbox"
msgid "Documents folder:"
msgstr "Složka s dokumenty:"

#: ui/main.qml:72
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save documents."
msgstr ""
"Tato složka bude použita jako výchozí pro otevírání či ukládání dokumentů."

#: ui/main.qml:80
#, kde-format
msgctxt "@label:textbox"
msgid "Downloads folder:"
msgstr "Složka pro stahování:"

#: ui/main.qml:86
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to save your downloaded items."
msgstr "Tato složka bude použita jako výchozí pro ukládání stažených věcí."

#: ui/main.qml:94
#, kde-format
msgctxt "@label:textbox"
msgid "Videos folder:"
msgstr "Složka s videi:"

#: ui/main.qml:100
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save video files."
msgstr ""
"Tato složka bude použita jako výchozí pro otevírání či ukládání video "
"souborů."

#: ui/main.qml:108
#, kde-format
msgctxt "@label:textbox"
msgid "Pictures folder:"
msgstr "Složka obrázků:"

#: ui/main.qml:114
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save image files."
msgstr ""
"Tato složka bude použita jako výchozí pro otevírání či ukládání souborů "
"obrázků."

#: ui/main.qml:122
#, kde-format
msgctxt "@label:textbox"
msgid "Music folder:"
msgstr "Složka s hudbou:"

#: ui/main.qml:128
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid "This folder will be used by default to open or save music files."
msgstr ""
"Tato složka bude použita jako výchozí pro otevírání či ukládání audio "
"souborů."

#: ui/main.qml:136
#, kde-format
msgctxt "@label:textbox"
msgid "Public folder:"
msgstr "Veřejná složka:"

#: ui/main.qml:142
#, kde-format
msgctxt "@info:tooltip and accessible description"
msgid ""
"This folder will be used by default for publicly-shared files when network "
"sharing is enabled."
msgstr ""
"Tato složka bude použita jako výchozí pro veřejně sdílené soubory pokud bude "
"povoleno sdílení na síti."

#: ui/main.qml:150
#, kde-format
msgctxt "@label:textbox"
msgid "Templates folder:"
msgstr "Složka se šablonami:"

#: ui/main.qml:156
#, kde-format
