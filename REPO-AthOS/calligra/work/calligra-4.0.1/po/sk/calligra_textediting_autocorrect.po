#: Autocorrect.cpp:26
#, kde-format
msgid "Configure &Autocorrection..."
msgstr "Nastaviť automatické opravy..."

#: Autocorrect.cpp:30
#, kde-format
msgid "Enable Autocorrection"
msgstr "Povoliť automatické opravy"

#: AutocorrectFactory.cpp:16
#, kde-format
msgid "Autocorrect"
msgstr "Automatické opravy"

#~ msgid "Si&mple Autocorrection"
#~ msgstr "Jednoduché automatické opravy"

#~ msgid ""
#~ "Detect when a new sentence is started and always ensure that the first "
#~ "character is an uppercase character"
#~ msgstr ""
#~ "Detekovať písanie novej vety a vždy sa uistiť, že prvý znak je napísaný "
#~ "veľkými písmenami"

#~ msgid ""
#~ "Convert &first letter of a sentence automatically to uppercase\n"
#~ "(e.g. \"my house. in this town\" to \"my house. In this town\")"
#~ msgstr ""
#~ "Konvertovať prvé písmeno vety automaticky na veľké písmená\n"
#~ "(napr. \"môj dom. v tomto meste\" na \"môj dom. V tomto meste\")"

#~ msgid ""
#~ "All words are checked for the common mistake of holding the shift key "
#~ "down a bit too long. If some words must have two uppercase characters, "
#~ "then those exceptions should be added in the 'Exceptions' tab."
#~ msgstr ""
#~ "Všetky slová sa kontrolujú na bežné chyby pri držaní shiftu príliš dlho. "
#~ "V niektorých slovách musia byť dve veľké písmená, tieto výnimky treba "
#~ "pridať do karty 'Výnimky'."

#~ msgid ""
#~ "Convert &two uppercase characters to one uppercase and one lowercase "
#~ "character\n"
#~ " (e.g. PErfect to Perfect)"
#~ msgstr ""
#~ "Konvertovať dve veľké písmená na jedno veľké a jedno malé písmeno\n"
#~ "(napr. SUper na Super)"

#~ msgid ""
#~ "Detect when a URL (Uniform Resource Locator) is typed and provide "
#~ "formatting that matches the way an Internet browser would show a URL."
#~ msgstr ""
#~ "Detekovať napísané URL a použiť formátovanie, ktoré sa zhoduje so "
#~ "spôsobom, ako by ho zobrazil prehliadač."

#~ msgid "Autoformat &URLs"
#~ msgstr "Automaticky formátovať URL"

#~ msgid ""
#~ "Make sure that more than one space cannot be typed, as this is a common "
#~ "mistake which is quite hard to find in formatted text."
#~ msgstr ""
#~ "Uistiť sa že sa nemôže napísať viac medzier ako jedna, toto je bežná "
#~ "chyba, ktorú je pomerne obtiažne nájsť vo formátovanom texte."

#~ msgid "&Suppress double spaces"
#~ msgstr "Potlačiť dvojité medzery"

#~ msgid ""
#~ "Keep correct formatting and indenting of sentences by automatically "
#~ "removing spaces typed at the beginning and end of a paragraph."
#~ msgstr ""
#~ "Zachovať správne formátovanie a odsadenie viet automatickým odstránením "
#~ "medzier napísaných na začiatku a konci odseku."

#~ msgid "R&emove spaces at the beginning and end of paragraphs"
#~ msgstr "Odstrániť medzery na začiatku a konci odsekov"

#~ msgid ""
#~ "When you use _underline_ or *bold*, the text between the underscores or "
#~ "asterisks will be converted to underlined or bold text."
#~ msgstr ""
#~ "Ak použijete _podčiarknutie_ alebo *tučné*, text medzi podčiarknutím "
#~ "alebo hviezdičkami sa skonvertuje na podčiarknutý alebo tučný."

#~ msgid "Automatically do &bold and underline formatting"
#~ msgstr "Automaticky formátovať tučné a podčiarknuté"

#~ msgid "Most standard fraction notations will be converted when available"
#~ msgstr "Najbežnejšie notácie zlomkov budú konvertované, ak je to možné"

#~ msgid "Re&place 1/2... with ½..."
#~ msgstr "&Nahradiť 1/2... s ½..."

#~ msgid ""
#~ "When typing '1)' or similar in front of a paragraph, automatically "
#~ "convert the paragraph to use that numbering style. This has the advantage "
#~ "that further paragraphs will also be numbered and the spacing is done "
#~ "correctly."
#~ msgstr ""
#~ "Pri napísaní '1)' a podobne na začiatku odseku, automaticky konvertovať "
#~ "odsek na použitie daného štýlu číslovania. Toto má výhodu, že aj ostatné "
#~ "odseky budú číslované a správne odsadené."

#~ msgid "Use &autonumbering for numbered paragraphs"
#~ msgstr "Použiť automatické číslovanie pre číslované odseky"

#~ msgid "Rep&lace 1st... with 1^st..."
#~ msgstr "Nahradiť 1st... s 1^st..."

#~ msgid "Capitalize name of days"
#~ msgstr "Názvy dní veľkým prvým písmenom"

#~ msgid ""
#~ "When typing '*' or '-' in front of a paragraph, automatically convert the "
#~ "paragraph to use that list-style. Using a list-style formatting means "
#~ "that a correct bullet is used to draw the list."
#~ msgstr ""
#~ "Pri napísaní '*' alebo '-' na začiatku osedku, automaticky konvertovať "
#~ "odsek na použitie daného štýlu zoznamu. To znamená, že sa použije správna "
#~ "odrážka na zapísanie zoznamu."

#~ msgid "Use l&ist-formatting for bulleted paragraphs"
#~ msgstr "Použiť formátovanie zoznamu pre odrážkové odseky"

#~ msgid "Default"
#~ msgstr "Predvolené"

#~ msgid "Custom &Quotes"
#~ msgstr "Vlastné úvodzovky"

#~ msgid "Replace &double quotes with typographical quotes"
#~ msgstr "Nahradiť dvojité úvodzovky typografickými úvodzovkami"

#~ msgid "Replace &single quotes with typographical quotes"
#~ msgstr "Nahradiť jednoduché úvodzovky typografickými úvodzovkami"

#~ msgid "Ad&vanced Autocorrection"
#~ msgstr "&Pokročilé automatické opravy"

#~ msgid "Replacements and exceptions for language:"
#~ msgstr "Náhrady a výnimky pre jazyk:"

#~ msgid "All Languages"
#~ msgstr "Všetky jazyky"

#~ msgid "&Enable word replacement"
#~ msgstr "Povoliť nahradzovanie slov"

#~ msgid "Replace &text with format"
#~ msgstr "Nahradiť text s formátom"

#~ msgid "&Find"
#~ msgstr "&Nájsť"

#~ msgid "..."
#~ msgstr "..."

#~ msgid "Re&place"
#~ msgstr "&Nahradiť"

#~ msgid "&Add"
#~ msgstr "&Pridať"

#~ msgid "Remove"
#~ msgstr "Odstrániť"

#~ msgid "C&hange Format..."
#~ msgstr "Zmeniť formát..."

#~ msgid "C&lear Format"
#~ msgstr "Vyčistiť formát"

#~ msgid "Find"
#~ msgstr "Nájsť"

#~ msgid "Replace"
#~ msgstr "Nahradiť"

#~ msgid "E&xceptions"
#~ msgstr "Vý&nimky"

#~ msgid "Do not treat as the end of a sentence:"
#~ msgstr "Nepovažovať za koniec vety:"

#~ msgid "Autoinclude"
#~ msgstr "Automaticky zahrnúť"

#~ msgid "R&emove"
#~ msgstr "&Odstrániť"

#~ msgid "Accept two uppercase letters in:"
#~ msgstr "Povoliť dve veľké písmená v:"

#~ msgid "A&dd"
#~ msgstr "Pri&dať"

#~ msgid "&Modify"
#~ msgstr "&Upraviť"

#~ msgid "Select Character"
#~ msgstr "Vybrať znak"

